prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}
libdir=${exec_prefix}/@CMAKE_INSTALL_LIBDIR@
includedir=${prefix}/@CMAKE_INSTALL_INCLUDEDIR@

Name: gamechannel
Version: @PROJECT_VERSION@

Description: A library for Xaya games with game channels.
URL: https://github.com/xaya/libxayagame

Requires: jsoncpp libjsonrpccpp-client libjsonrpccpp-server libglog sqlite3 protobuf channelcore libxayagame libxayautil

Cflags: -I${includedir}
Libs: -L${libdir} -lgamechannel
